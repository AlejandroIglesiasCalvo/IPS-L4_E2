CREATE MEMORY TABLE PUBLIC.ENCARGO(ID_PRODUCTO VARCHAR(9) NOT NULL,ID_PEDIDO VARCHAR(9) NOT NULL,UNIDADES VARCHAR(9) NOT NULL,CONSTRAINT PK_ENCARGO PRIMARY KEY(ID_PRODUCTO,ID_PEDIDO),CONSTRAINT FK_ENCARGO_PRODUCTO FOREIGN KEY(ID_PRODUCTO) REFERENCES PUBLIC.PRODUCTO(ID_PRODUCTO),CONSTRAINT FK_ENCARGO_PEDIDO FOREIGN KEY(ID_PEDIDO) REFERENCES PUBLIC.PEDIDO(ID_PEDIDO))
CREATE MEMORY TABLE PUBLIC.VENTA(ID_VENTA VARCHAR(9) NOT NULL,ID_PRESUPUESTO VARCHAR(9) NOT NULL,FECHA DATE NOT NULL,TOTAL VARCHAR(9) NOT NULL,MONTADOS VARCHAR(9) NOT NULL,CONSTRAINT PK_VENTA PRIMARY KEY(ID_VENTA),CONSTRAINT FK_PRESUPUESTO_VENTA FOREIGN KEY(ID_PRESUPUESTO) REFERENCES PUBLIC.PRESUPUESTO(ID_PRESUPUESTO))
